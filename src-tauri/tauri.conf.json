{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "GestionGastos",
  "version": "0.1.0",
  "identifier": "com.gestiongastos.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173/duplicados/gastos/",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Gesti√≥n de Gastos",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "plugins": {
    "shell": {
      "open": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
